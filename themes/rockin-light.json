    "statusBar.noFolderBackground": "#eee",
    "statusBar.noFolderBorder":"#aaa",

    "statusBar.foreground": "#666",
    "statusBar.background": "#eee",
    "statusBar.border":"#aaa",

    "statusBar.debuggingForeground": "#eee",
    "statusBar.debuggingBackground": "#2196f3",
    "statusBar.debuggingBorder":"#aaa",

