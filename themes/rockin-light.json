    // INTEGRATED TERMINAL
    ////////////////////////////////////////////////////////////////
    "terminal.foreground": "#00e",
    "terminal.background": "#eee",
    "terminalCursor.foreground":"#f00",
    "terminalCursor.background":"#fff",

    ////////////////////////////////////////////////////////////////
    // VARIOUS
    ////////////////////////////////////////////////////////////////
    "settings.modifiedItemIndicator":"#2196f3",
    "settings.headerForeground": "#000",
    "settings.dropdownBackground": "#eee",
    "settings.checkboxBackground": "#eee",
    "settings.textInputBackground": "#eee",
    "settings.numberInputBackground": "#eee",
    "badge.foreground": "#2196f3",
    "badge.background": "#eee"
