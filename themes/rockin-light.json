    "button.foreground": "#fff",
    "button.background": "#2196f3",
    "button.hoverBackground": "#5be",
    "button.secondaryForeground": "#000",
    "button.secondaryBackground": "#2196f3",
    "button.secondaryHoverBackground": "#5be",
    "checkbox.foreground": "#000",
    "checkbox.background": "#aaa",
    "checkbox.border": "#666",

    "selection.background": "#2196f3",
    "dropdown.foreground": "#000",
    "dropdown.background": "#eee",
    "dropdown.listBackground": "#eee",
    "dropdown.border": "#aaa",

    "widget.shadow": "#0000",
    // FIND WIDGET OPTION BUTTONS (match case, match word, regex, etc)
    "input.background": "#eeec",
    "input.border": "#aaaa",
    "input.placeholderForeground": "#aaa",

    "inputOption.activeForeground": "#000",
    "inputOption.activeBackground": "#eee",
    "inputOption.activeBorder": "#000",

    "inputValidation.errorForeground": "#f00",
    "inputValidation.errorBackground": "#fee",

    ////////////////////////////////////////////////////////////////
    // CODE EDITOR
    ////////////////////////////////////////////////////////////////
    "editor.foreground": "#000", // non-symbol code color
    "editor.background": "#eee", // negative space space color

    // BORDER SEPARATING EDITOR FROM TABS
    "editorGroupHeader.tabsBorder": "#aaa",
    "editorGroupHeader.tabsBackground": "#eee",

    "editorCursor.foreground":"#f00",
    "editorCursor.background":"#000",
    "editorBracketMatch.border": "#2196f366",
    "editorBracketMatch.background":"#eee",

    "editor.selectionForeground": "#fff", // selected text color
    "editor.selectionBackground": "#2196f370", // selected text background color
    "editor.inactiveSelectionBackground":"#999",

    "editor.selectionHighlightBorder": "#2196f3",
    "editor.selectionHighlightBackground": "#0000",

    "editor.rangeHighlightBorder":"#2196f3",
    "editor.findMatchBorder": "#0ff",
    "editor.findMatchBackground":"#0ff",
    "editor.findMatchHighlightBackground": "#aaa5",
    "editor.findMatchHighlightBorder": "#f00",

    // EDITOR WHITE SPACE, RULERS AND INDENTATION GUIDES
    "editorRuler.foreground": "#ccca",
    "editorWhitespace.foreground": "#aaa6",
    "editorIndentGuide.background": "#aaa6",
    "editorIndentGuide.activeBackground": "#0006",

    "editorWidget.foreground": "#000c",
    "editorWidget.background": "#eeed",
    "editorWidget.border": "#aaac",

    // HOVER WIDGET
    "editorHoverWidget.background":"#eee",
    "editorHoverWidget.border": "#aaa",

    // SUGGESTION WIDGET (AUTOCOMPLETE, SYMBOL LOOKUP, ETC)
    "editorSuggestWidget.background": "#eee",
    "editorSuggestWidget.border":"#aaa",

    // LINE NUMBER
    "editorLineNumber.activeForeground": "#000",
    "editorLineNumber.foreground": "#bbb",
    "editor.lineHighlightBorder": "#aaa",

    "editorGutter.background": "#eee",

    // SCROLL BARS
    "scrollbarSlider.background":"#0005",
    "scrollbarSlider.hoverBackground": "#0008",
    "scrollbarSlider.activeBackground": "#000a",

    // MINIMAP
    "minimap.background": "#eeea",
    "minimapSlider.background": "#ccc6",
    "minimapSlider.hoverBackground": "#aaa6",
    "minimapSlider.activeBackground": "#8886",

    "minimap.findMatchHighlight": "#2196f3",
    "minimap.selectionHighlight":  "#2196f3",
    // hide error & warning minimap indicators
    "minimap.errorHighlight":  "#f000",
    "minimap.warningHighlight": "#f000",

    // EDITOR CODE LINTING
    "editorError.foreground": "#f00",
    // "editorError.border": "#f00",
    "editorWarning.foreground": "#fb0",
    // "editorWarning.border": "#fb0",
    "editorInfo.foreground": "#0cf",
    // "editorInfo.border": "#0cf",
    "editorHint.foreground": "#0f0",
    // "editorHint.border": "#aaa",

    // TABS
    // "tab.activeBorder": "#2196f3",
    "tab.activeBorderTop": "#2196f3",
    "tab.activeBackground": "#eee",
    "tab.border": "#aaa",
    "tab.activeForeground": "#000",
    "tab.inactiveForeground": "#888",
    "tab.inactiveBackground": "#eee",
    "tab.hoverBackground":"#eee",

    // BREADCRUMBS
    "breadcrumb.foreground": "#888",
    "breadcrumb.background": "#eee",
    "breadcrumb.focusForeground": "#2196f3",
    "breadcrumb.activeSelectionForeground": "#000",
    "breadcrumbPicker.background": "#eee",

    ////////////////////////////////////////////////////////////////
    // DIFF EDITOR
    ////////////////////////////////////////////////////////////////
    "diffEditor.diagonalFill": "#ccc",
    "diffEditor.border": "#aaa",
    "diffEditor.insertedTextBackground": "#0f03",
    "diffEditor.insertedTextBorder": "#0000",
    "diffEditor.removedTextBackground": "#f003",
    "diffEditor.removedTextBorder": "#0000",

    ////////////////////////////////////////////////////////////////
    // SIDE BAR
    ////////////////////////////////////////////////////////////////
    "sideBar.background": "#eee",
    // foreground color for sidebar elements without a special Git status
    "sideBar.foreground": "#222",
    "sideBarTitle.foreground": "#777",
    "sideBarSectionHeader.foreground": "#000",
    "sideBarSectionHeader.background": "#eee",
    "sideBarSectionHeader.border": "#aaa",

    // explorer tree item styles
    "list.activeSelectionForeground": "#000",
    "list.activeSelectionBackground": "#ccc",
    "list.focusOutline": "#0000",
    "list.inactiveSelectionForeground": "#000",
    "list.inactiveSelectionBackground": "#ccc",
    "list.inactiveFocusOutline": "#0000",
    "list.hoverForeground": "#000",
    "list.hoverBackground": "#ddd",
    "list.focusBackground": "#eee",
    "list.highlightForeground": "#2196f3",

    // Git status colors
    "gitDecoration.modifiedResourceForeground": "#f90",
    "gitDecoration.addedResourceForeground": "#0cf",
    "gitDecoration.deletedResourceForeground": "#f00",
    "gitDecoration.untrackedResourceForeground": "#909",
    "gitDecoration.ignoredResourceForeground": "#999",
    "gitDecoration.conflictingResourceForeground": "#80f",
    "gitDecoration.submoduleResourceForeground": "#00f",

